{
  "name": "igomon-app",
  "version": "1.0.0",
  "description": "囲碁アンケートサイト『いごもん』",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev:client": "cd client && vite",
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "build:client": "cd client && vite build",
    "build:server": "tsc",
    "start": "node start.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,css,scss,json,md}\" --ignore-path .prettierignore"
  },
  "keywords": [
    "go",
    "igo",
    "questionnaire",
    "survey"
  ],
  "author": "igomon team",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@prisma/client": "^6.10.1",
    "@sabaki/sgf": "^3.4.7",
    "@sabaki/go-board": "^1.4.3",
    "canvas": "^3.1.2",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "prisma": "^6.10.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sabaki__sgf": "^3.4.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  }
}
